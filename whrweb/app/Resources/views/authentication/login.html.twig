{% extends 'base.html.twig' %}

{% block body %}
	{{ form_start( form ) }}
		{{ form_errors( form ) }}
		{{ form_row( form._username, { 'value': last_username } ) }}
		{{ form_row( form._password ) }}
		{% if form._captcha is defined %}
			{{ form_row( form._captcha ) }}
		{% endif %}
		{{ form_row( form.login ) }}
	{{ form_end( form ) }}
	<p></p>
{% endblock %}